{
  "name": "interra-sveltekit",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "svelte-kit sync && svelte-kit build",
    "dev": "svelte-kit dev",
    "preview": "svelte-kit preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@sveltejs/adapter-netlify": "^4.0.0",
    "@sveltejs/kit": "^2.47.1",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@tailwindcss/typography": "^0.5.13",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2",
    "vite": "^5.4.10"
  },
  "dependencies": {
    "@datocms/cda-client": "^0.2.7",
    "datocms-client": "^3.0.0"
  }
}
